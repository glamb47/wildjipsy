{%- if section.settings.visible and section.blocks.size > 0 -%}

    {%- assign content_width = 'container-fullwidth' -%}

    <section id="{{ section.settings.custom_id }}"
             class="section section-full-screen-banner section-slider p-relative {{ section.settings.additional_classes }}"
             js-fill-height>

        {%- include 'slider-section' -%}

        <button js-fullscreen-banner-button
                class="js-fullscreen-banner-button btn-link m-4 align-bottom-center z-index-999"
                style="color:{{ section.settings.button_color }}">
            <i class="icon-arrow-down-drop-circle-outline s-14"></i>
        </button>
    </section>

{%- endif -%}

