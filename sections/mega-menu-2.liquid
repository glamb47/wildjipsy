{%- capture section_style -%}
background-size: {{ section.settings.bg_size }}; background-repeat: {{ section.settings.bg_repeat }}; background-position: {{ section.settings.bg_position }};
{% if section.settings.bg_image != blank %}background-image: url({{ section.settings.bg_image | img_url: '2048x' }});{% endif %}
{% if section.settings.bg_color != blank %}background-color: {{ section.settings.bg_color }};{% endif %}
{%- endcapture -%}

{% if section.settings.bg_color == blank or section.settings.bg_color == 'rgba(0,0,0,0)' %}{% assign bg_color = '#ffffff' %}{% else %}{% assign bg_color = section.settings.bg_color %}{% endif %}
{%- if section.settings.bg_color_type == 'auto' -%}
    {%- include 'util-color-brightness' color: bg_color, prepend: 'mega-menu-', append: '-bg' -%}
{%- else -%}
    {%- assign color_brightness = section.settings.bg_color_type -%}
{%- endif -%}

<script type="text/js-template" id="'mega-menu-2'" js-template js-set-window-mega-menu-settings
        mega-menu-content-width="'{{ section.settings.content_width }}'"
        mega-menu-title-link="'{{ section.settings.title_link | default: '#' }}'">
    {%- include 'mega-menu' -%}
</script>

<script type="text/javascript">
    window.jasper.megamenus['mega-menu-2'] = {
        content_width: '{{ section.settings.content_width }}',
        title_link   : '{{ section.settings.title_link | default: '#' }}'
    }
</script>

