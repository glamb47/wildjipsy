{%- assign content_width = section.settings.content_width | default: settings.content_width -%}

{%- include 'util-section-bg-color'
auto_bg_color: section.settings.auto_bg_color,
bg_color: section.settings.bg_color,
bg_opacity: section.settings.bg_opacity -%}

{%- capture section_style -%}
background-size: {{ section.settings.bg_size }}; background-repeat: {{ section.settings.bg_repeat }}; background-position: {{ section.settings.bg_position }};
{%- if section.settings.bg_image != blank -%}background-image: url({{ section.settings.bg_image | img_url: '1920x' }});{%- endif -%}
background-color: {{ section_bg_color }};
{%- endcapture -%}

{%- if section.settings.bg_color_type == 'auto' -%}
    {%- include 'util-color-brightness' color: section_bg_color, append: '-bg' -%}
{%- else -%}
    {%- assign color_brightness = section.settings.bg_color_type -%}
{%- endif -%}

{%- capture section_content -%}

<section id="{{ section.settings.custom_id }}"
         class="section section-custom section-h-{{ section.settings.section_height }} {{ section.settings.additional_classes }}  {{ section.settings.show_only_desktop_or_mobile }} {{ color_brightness }} {% if section.settings.bg_parallax == 'enable' %}parallax-bg{% endif %}"
         style="{{ section_style }}"
         {% if section.settings.section_height == "window-height" %}js-fill-height{% endif %}>

    <div class="section-content animate-on-scroll {{ content_width }}">

        <div
            class="section-title text-uppercase flex-items-middle content-style-{{ section.settings.content_style }} {{ section.settings.arrows_position }}">

            {%- if section.settings.show_title and section.settings.title != blank -%}

                <h3 class="{{ section.settings.title_alignment }} w-100 m-0 py-12">
                    {{ section.settings.title }}
                </h3>

            {%- endif -%}

            {%- if section.settings.content_style == 'carousel' and section.settings.arrows -%}

                {%- if section.settings.arrows_position == 'arrows-top-left' or section.settings.arrows_position == 'arrows-top-right' -%}

                    <div class="arrows-wrapper flex-nogrow flex-items-middle">
                        <button class="flickity-prev-next-button previous small" type="button" aria-label="previous"
                                bc-flickity-previous bc-flickity-id="index-carousel-{{ section.id }}">
                            <svg viewBox="0 0 100 100">
                                <path d="M 10,50 L 60,100 L 70,90 L 30,50  L 70,10 L 60,0 Z" class="arrow"></path>
                            </svg>
                        </button>

                        <button class="flickity-prev-next-button next small" type="button" aria-label="next"
                                bc-flickity-next bc-flickity-id="index-carousel-{{ section.id }}">
                            <svg viewBox="0 0 100 100">
                                <path d="M 10,50 L 60,100 L 70,90 L 30,50  L 70,10 L 60,0 Z" class="arrow"
                                      transform="translate(100, 100) rotate(180) "></path>
                            </svg>
                        </button>
                    </div>

                {%- endif -%}

            {%- endif -%}

        </div>

        <div
            class="custom-content {% if section.settings.content_style != 'carousel' %} row{% endif %}{% if section.settings.content_style == 'masonry' %} grid-masonry {% endif %}">

            {%- if section.settings.content_style == 'carousel' -%}

            <div
                class="carousel {{ section.settings.carousel_style }} {{ section.settings.arrows_style }}"
                bc-flickity-id="index-carousel-{{ section.id }}"
                bc-flickity='{
                        "cellAlign": "left",
                        {% if section.settings.carousel_style == "simplified" or section.settings.carousel_style == "simplified-nospace" %}"cellAlign": "center",{% endif %}
                        "autoPlay": {% if section.settings.autoplay == true %} {{ section.settings.autoplaySpeed }}{% else %}false{% endif %},
                        "selectedAttraction":{{ section.settings.selectedAttraction }},
                        "friction": {{ section.settings.friction }},
                        "wrapAround": {{ section.settings.infinite }},
                        "prevNextButtons": {% if section.settings.arrows_position == 'arrows-inside' %}{{ section.settings.arrows }}{% else %}false{% endif %},
                        "pageDots": {{ section.settings.dots }},
                        "groupCells": {{ section.settings.groupCells }},
                        "draggable": true,
                        "dragThreshold": 15,
                        {% if section.settings.carousel_style == "simplified" or section.settings.carousel_style == "simplified-nospace" %}"percentPosition": false,{% endif %}
                        "contain": true,
                        "resize": true,
                        "imagesLoaded": true
                          }'
                js-flickity-carousel-fix
                {% if section.settings.carousel_style == "simplified" or section.settings.carousel_style == "simplified-nospace" %}js-flickity-simplified-carousel {% endif %}>

                {%- endif -%}

                {%- for block in section.blocks -%}

                    {%- assign column_width = block.settings.width -%}

                    {%- include 'util-column-class' -%}

                    {%- assign item_class = column_class | prepend: 'carousel-item ' -%}

                    <div
                        class="{{ item_class }} {{ block.settings.align_text }} {{ block.settings.alignment_v }} {{ block.settings.alignment_h }} {{ block.settings.additional_classes }}" {{ block.shopify_attributes }}>

                        {%- assign block_width = '' -%}

                        {%- if block.type != 'image' and block.type != 'image_with_caption' -%}
                            {%- assign block_width = 'w-100' -%}
                        {%- endif -%}

                        <div
                            class="block-type-{{ block.type }} {{ block_width }}"
                            style="max-width:{{ block.settings.content_inner_max_width }}">

                            {%- case block.type -%}

                            {%- comment -%}
                            {# @ Image Block #}
                            {%- endcomment -%}
                            {%- when 'image' -%}

                            <div class="image-wrapper">

                                {% if block.settings.url != blank %}<a href="{{ block.settings.url }}">{% endif %}

                                    {%- if block.settings.image != blank -%}

                                        <img class="img-fluid"
                                             src="{{ block.settings.image | img_url: block.settings.image_size }}"
                                             alt="{{ block.settings.image.alt | escape }}">

                                        {%- if block.settings.hover_title != blank or block.settings.hover_text != blank -%}
                                            <div class="border"></div>
                                            <div class="overlay">
                                                <div class="text-uppercase h3 text-center pb-4">
                                                    {{ block.settings.hover_title }}
                                                </div>

                                                <div class="text-center">
                                                    {{ block.settings.hover_text }}
                                                </div>
                                            </div>
                                        {%- endif -%}

                                    {%- else -%}

                                        {{ 'image' | placeholder_svg_tag: 'placeholder-svg img-fluid w-100' }}

                                    {%- endif -%}

                                    {% if block.settings.url != blank %}</a>{% endif %}
                            </div>

                            {%- comment -%}
                            {# @ Image With Caption Block #}
                            {%- endcomment -%}
                            {%- when 'image_with_caption' -%}

                            <div class="image-with-caption {{ block.settings.fg_color }}">

                                {%- if block.settings.image != blank -%}

                                    <img class="caption-image img-fluid"
                                         src="{{ block.settings.image | img_url: block.settings.image_size }}"
                                         alt="{{ block.settings.image.alt | escape }}">

                                {%- else -%}

                                    {{ 'image' | placeholder_svg_tag: 'placeholder-svg img-fluid w-100' }}

                                {%- endif -%}

                                {%- if block.settings.caption -%}

                                    {%- if block.settings.url != blank -%}
                                        <a href="{{ block.settings.url }}">
                                    {%- endif -%}

                                    <div class="caption-area {{ block.settings.caption_alignment_h }}">
                                        <div
                                            class="caption-wrapper {{ block.settings.caption_alignment_text }} {{ block.settings.caption_alignment_v }} ">
                                            <div class="caption">
                                                {{ block.settings.caption }}
                                            </div>
                                        </div>
                                    </div>

                                    {% if block.settings.url != blank %}</a>{% endif %}

                                {%- endif -%}

                            </div>

                            {%- when 'text' -%}

                            {%- if block.settings.title != blank -%}
                                <div class="h3">{{ block.settings.title | escape }}</div>
                            {%- endif -%}

                            {%- if block.settings.text != blank -%}
                                <div class="rte-setting">{{ block.settings.text }}</div>
                            {%- endif -%}

                            {%- comment -%}
                            {# @ Video Block #}
                            {%- endcomment -%}
                            {%- when 'video' -%}

                            <div class="video-wrapper">

                                {%- if block.settings.video_url == blank -%}

                                    <iframe src="//www.youtube.com/embed/_9VUPq3SxOc?rel=0&showinfo=0&vq=720"
                                            width="850" height="480" frameborder="0" allowfullscreen></iframe>

                                {%- else -%}

                                    {%- if block.settings.video_url.type == "youtube" -%}

                                        <iframe
                                            src="//www.youtube.com/embed/{{ block.settings.video_url.id }}?rel=0&showinfo=0&vq=720"
                                            width="850" height="480" frameborder="0" allowfullscreen></iframe>
                                    {%- endif -%}

                                    {%- if block.settings.video_url.type == "vimeo" -%}
                                        <iframe
                                            src="//player.vimeo.com/video/{{ block.settings.video_url.id }}?byline=0&portrait=0&badge=0"
                                            width="850" height="480" frameborder="0" allowfullscreen></iframe>
                                    {%- endif -%}

                                {%- endif -%}
                            </div>

                            {%- comment -%}
                            {# @ Product Block #}
                            {%- endcomment -%}
                            {%- when 'product' -%}

                            {%- assign product_image_ratio = settings.product_image_aspect_ratio_w | plus :0.0 | divided_by: settings.product_image_aspect_ratio_h -%}
                            {%- include 'util-image-size' column_width: block.settings.width, image_ratio: product_image_ratio, custom_ratio: settings.enable_product_custom_aspect_ratio -%}

                            {%- include 'product-item'
                            product: all_products[block.settings.product],
                            hover_bg: section.settings.hover_bg,
                            style : block.settings.product_style,
                            sale_badge_style : block.settings.product_sale_badge_style,
                            hover_buttons_style : block.settings.product_hover_buttons_style,
                            show_star_rating: block.settings.product_show_star_rating,
                            image_size: image_size -%}

                            {%- comment -%}
                            {# @ Collection Products #}
                            {%- endcomment -%}
                            {%- when 'collection_products' -%}

                            {%- assign collection = collections[block.settings.collection] -%}

                            {%- include 'util-column-class-2' column_count_2: block.settings.column_count, column_min_2: 2 -%}

                            {%- assign product_image_ratio = settings.product_image_aspect_ratio_w | plus :0.0 | divided_by: settings.product_image_aspect_ratio_h -%}
                            {%- include 'util-image-size' column_count: block.settings.column_count, image_ratio: product_image_ratio, custom_ratio: settings.enable_product_custom_aspect_ratio -%}

                            {% capture item_class_2 %}{{ column_class_2 }}{% endcapture %}

                            {%- assign product_limit = false -%}
                            {%- if block.settings.product_limit != blank -%}
                                {%- assign product_limit = block.settings.product_limit -%}
                            {%- endif -%}

                            {%- if collection.products.size > 0 -%}
                                <div class="row">
                                    {%- for product in collection.products limit: product_limit -%}

                                        {%- include 'product-item'
                                        product: product,
                                        style: block.settings.product_style,
                                        item_class: item_class_2,
                                        sale_badge_style : block.settings.product_sale_badge_style,
                                        hover_buttons_style : block.settings.product_hover_buttons_style,
                                        hover_bg: section.settings.hover_bg,
                                        show_star_rating: block.settings.product_show_star_rating,
                                        image_size: image_size -%}
                                    {%- endfor -%}
                                </div>
                            {%- endif -%}

                            {%- comment -%}
                            {# @ Collection Block #}
                            {%- endcomment -%}
                            {%- when 'collection' -%}

                            {%- assign collection_image_ratio = settings.collection_image_aspect_ratio_w | plus :0.0 | divided_by: settings.collection_image_aspect_ratio_h -%}
                            {%- include 'util-image-size' column_width: block.settings.width, image_ratio: collection_image_ratio, custom_ratio: settings.enable_collection_custom_aspect_ratio -%}

                            {%- include 'collection-item'
                            collection: collections[block.settings.collection],
                            collection_image_size: image_size -%}

                            {%- comment -%}
                            {# @ Map Block #}
                            {%- endcomment -%}
                            {%- when 'map' -%}

                            {%- if block.settings.address != '' -%}

                                <div map-lazy-load="https://maps.google.com/maps/api/js"
                                    {%- if settings.google_maps_api_key != '' -%}
                                    map-lazy-load-params="https://maps.googleapis.com/maps/api/js?key={{ settings.google_maps_api_key }}"
                                    {%- endif -%}>

                                    <ng-map center="{{ block.settings.address }}"
                                            zoom="{{ block.settings.zoom }}"
                                        {%- if block.settings.map_type == 'satellite' -%}
                                            map-type-id="MapTypeId.SATELLITE"
                                        {%- endif -%}
                                        {%- case block.settings.map_style -%}
                                        {% when 'ultra light' %}
                                            styles="[ { 'featureType': 'water', 'elementType': 'geometry', 'stylers': [ { 'color': '#e9e9e9' }, { 'lightness': 17 } ] }, { 'featureType': 'landscape', 'elementType': 'geometry', 'stylers': [ { 'color': '#f5f5f5' }, { 'lightness': 20 } ] }, { 'featureType': 'road.highway', 'elementType': 'geometry.fill', 'stylers': [ { 'color': '#ffffff' }, { 'lightness': 17 } ] }, { 'featureType': 'road.highway', 'elementType': 'geometry.stroke', 'stylers': [ { 'color': '#ffffff' }, { 'lightness': 29 }, { 'weight': 0.2 } ] }, { 'featureType': 'road.arterial', 'elementType': 'geometry', 'stylers': [ { 'color': '#ffffff' }, { 'lightness': 18 } ] }, { 'featureType': 'road.local', 'elementType': 'geometry', 'stylers': [ { 'color': '#ffffff' }, { 'lightness': 16 } ] }, { 'featureType': 'poi', 'elementType': 'geometry', 'stylers': [ { 'color': '#f5f5f5' }, { 'lightness': 21 } ] }, { 'featureType': 'poi.park', 'elementType': 'geometry', 'stylers': [ { 'color': '#dedede' }, { 'lightness': 21 } ] }, { 'elementType': 'labels.text.stroke', 'stylers': [ { 'visibility': 'on' }, { 'color': '#ffffff' }, { 'lightness': 16 } ] }, { 'elementType': 'labels.text.fill', 'stylers': [ { 'saturation': 36 }, { 'color': '#333333' }, { 'lightness': 40 } ] }, { 'elementType': 'labels.icon', 'stylers': [ { 'visibility': 'off' } ] }, { 'featureType': 'transit', 'elementType': 'geometry', 'stylers': [ { 'color': '#f2f2f2' }, { 'lightness': 19 } ] }, { 'featureType': 'administrative', 'elementType': 'geometry.fill', 'stylers': [ { 'color': '#fefefe' }, { 'lightness': 20 } ] }, { 'featureType': 'administrative', 'elementType': 'geometry.stroke', 'stylers': [ { 'color': '#fefefe' }, { 'lightness': 17 }, { 'weight': 1.2 } ] } ]"
                                        {% when 'subtle grayscale' %}
                                            styles="[{'featureType': 'administrative', 'elementType': 'all', 'stylers': [{'saturation': '-100'}]}, {'featureType': 'administrative.province', 'elementType': 'all', 'stylers': [{'visibility': 'off'}]}, {'featureType': 'landscape', 'elementType': 'all', 'stylers': [{'saturation': -100}, {'lightness': 65}, {'visibility': 'on'}]}, {'featureType': 'poi', 'elementType': 'all', 'stylers': [{'saturation': -100}, {'lightness': '50'}, {'visibility': 'simplified'}]}, {'featureType': 'road', 'elementType': 'all', 'stylers': [{'saturation': '-100'}]}, {'featureType': 'road.highway', 'elementType': 'all', 'stylers': [{'visibility': 'simplified'}]}, {'featureType': 'road.arterial', 'elementType': 'all', 'stylers': [{'lightness': '30'}]}, {'featureType': 'road.local', 'elementType': 'all', 'stylers': [{'lightness': '40'}]}, {'featureType': 'transit', 'elementType': 'all', 'stylers': [{'saturation': -100}, {'visibility': 'simplified'}]}, {'featureType': 'water', 'elementType': 'geometry', 'stylers': [{'hue': '#ffff00'}, {'lightness': -25}, {'saturation': -97}]}, {'featureType': 'water', 'elementType': 'labels', 'stylers': [{'lightness': -25}, {'saturation': -100}]}]"
                                        {% when 'shades of grey' %}
                                            styles="[{'featureType': 'all', 'elementType': 'labels.text.fill', 'stylers': [{'saturation': 36}, {'color': '#000000'}, {'lightness': 40}]}, {'featureType': 'all', 'elementType': 'labels.text.stroke', 'stylers': [{'visibility': 'on'}, {'color': '#000000'}, {'lightness': 16}]}, {'featureType': 'all', 'elementType': 'labels.icon', 'stylers': [{'visibility': 'off'}]}, {'featureType': 'administrative', 'elementType': 'geometry.fill', 'stylers': [{'color': '#000000'}, {'lightness': 20}]}, {'featureType': 'administrative', 'elementType': 'geometry.stroke', 'stylers': [{'color': '#000000'}, {'lightness': 17}, {'weight': 1.2}]}, {'featureType': 'landscape', 'elementType': 'geometry', 'stylers': [{'color': '#000000'}, {'lightness': 20}]}, {'featureType': 'poi', 'elementType': 'geometry', 'stylers': [{'color': '#000000'}, {'lightness': 21}]}, {'featureType': 'road.highway', 'elementType': 'geometry.fill', 'stylers': [{'color': '#000000'}, {'lightness': 17}]}, {'featureType': 'road.highway', 'elementType': 'geometry.stroke', 'stylers': [{'color': '#000000'}, {'lightness': 29}, {'weight': 0.2}]}, {'featureType': 'road.arterial', 'elementType': 'geometry', 'stylers': [{'color': '#000000'}, {'lightness': 18}]}, {'featureType': 'road.local', 'elementType': 'geometry', 'stylers': [{'color': '#000000'}, {'lightness': 16}]}, {'featureType': 'transit', 'elementType': 'geometry', 'stylers': [{'color': '#000000'}, {'lightness': 19}]}, {'featureType': 'water', 'elementType': 'geometry', 'stylers': [{'color': '#000000'}, {'lightness': 17}]}]"
                                        {% when 'blue water' %}
                                            styles="[{'featureType': 'administrative', 'elementType': 'labels.text.fill', 'stylers': [{'color': '#444444'}]}, {'featureType': 'landscape', 'elementType': 'all', 'stylers': [{'color': '#f2f2f2'}]}, {'featureType': 'poi', 'elementType': 'all', 'stylers': [{'visibility': 'off'}]}, {'featureType': 'road', 'elementType': 'all', 'stylers': [{'saturation': -100}, {'lightness': 45}]}, {'featureType': 'road.highway', 'elementType': 'all', 'stylers': [{'visibility': 'simplified'}]}, {'featureType': 'road.arterial', 'elementType': 'labels.icon', 'stylers': [{'visibility': 'off'}]}, {'featureType': 'transit', 'elementType': 'all', 'stylers': [{'visibility': 'off'}]}, {'featureType': 'water', 'elementType': 'all', 'stylers': [{'color': '#46bcec'}, {'visibility': 'on'}]}]"
                                        {% when 'light dream' %}
                                            styles="[{'featureType': 'landscape', 'stylers': [{'hue': '#FFBB00'}, {'saturation': 43.400000000000006}, {'lightness': 37.599999999999994}, {'gamma': 1}]}, {'featureType': 'road.highway', 'stylers': [{'hue': '#FFC200'}, {'saturation': -61.8}, {'lightness': 45.599999999999994}, {'gamma': 1}]}, {'featureType': 'road.arterial', 'stylers': [{'hue': '#FF0300'}, {'saturation': -100}, {'lightness': 51.19999999999999}, {'gamma': 1}]}, {'featureType': 'road.local', 'stylers': [{'hue': '#FF0300'}, {'saturation': -100}, {'lightness': 52}, {'gamma': 1}]}, {'featureType': 'water', 'stylers': [{'hue': '#0078FF'}, {'saturation': -13.200000000000003}, {'lightness': 2.4000000000000057}, {'gamma': 1}]}, {'featureType': 'poi', 'stylers': [{'hue': '#00FF6A'}, {'saturation': -1.0989010989011234}, {'lightness': 11.200000000000017}, {'gamma': 1}]}]"
                                        {% when 'wy' %}
                                            styles="[{'featureType': 'all', 'elementType': 'geometry.fill', 'stylers': [{'weight': '2.00'}]}, {'featureType': 'all', 'elementType': 'geometry.stroke', 'stylers': [{'color': '#9c9c9c'}]}, {'featureType': 'all', 'elementType': 'labels.text', 'stylers': [{'visibility': 'on'}]}, {'featureType': 'landscape', 'elementType': 'all', 'stylers': [{'color': '#f2f2f2'}]}, {'featureType': 'landscape', 'elementType': 'geometry.fill', 'stylers': [{'color': '#ffffff'}]}, {'featureType': 'landscape.man_made', 'elementType': 'geometry.fill', 'stylers': [{'color': '#ffffff'}]}, {'featureType': 'poi', 'elementType': 'all', 'stylers': [{'visibility': 'off'}]}, {'featureType': 'road', 'elementType': 'all', 'stylers': [{'saturation': -100}, {'lightness': 45}]}, {'featureType': 'road', 'elementType': 'geometry.fill', 'stylers': [{'color': '#eeeeee'}]}, {'featureType': 'road', 'elementType': 'labels.text.fill', 'stylers': [{'color': '#7b7b7b'}]}, {'featureType': 'road', 'elementType': 'labels.text.stroke', 'stylers': [{'color': '#ffffff'}]}, {'featureType': 'road.highway', 'elementType': 'all', 'stylers': [{'visibility': 'simplified'}]}, {'featureType': 'road.arterial', 'elementType': 'labels.icon', 'stylers': [{'visibility': 'off'}]}, {'featureType': 'transit', 'elementType': 'all', 'stylers': [{'visibility': 'off'}]}, {'featureType': 'water', 'elementType': 'all', 'stylers': [{'color': '#46bcec'}, {'visibility': 'on'}]}, {'featureType': 'water', 'elementType': 'geometry.fill', 'stylers': [{'color': '#c8d7d4'}]}, {'featureType': 'water', 'elementType': 'labels.text.fill', 'stylers': [{'color': '#070707'}]}, {'featureType': 'water', 'elementType': 'labels.text.stroke', 'stylers': [{'color': '#ffffff'}]}]"
                                        {% when 'lost in desert' %}
                                            styles="[{'elementType': 'labels', 'stylers': [{'visibility': 'off'}, {'color': '#f49f53'}]}, {'featureType': 'landscape', 'stylers': [{'color': '#f9ddc5'}, {'lightness': -7}]}, {'featureType': 'road', 'stylers': [{'color': '#813033'}, {'lightness': 43}]}, {'featureType': 'poi.business', 'stylers': [{'color': '#645c20'}, {'lightness': 38}]}, {'featureType': 'water', 'stylers': [{'color': '#1994bf'}, {'saturation': -69}, {'gamma': 0.99}, {'lightness': 43}]}, {'featureType': 'road.local', 'elementType': 'geometry.fill', 'stylers': [{'color': '#f19f53'}, {'weight': 1.3}, {'visibility': 'on'}, {'lightness': 16}]}, {'featureType': 'poi.business'}, {'featureType': 'poi.park', 'stylers': [{'color': '#645c20'}, {'lightness': 39}]}, {'featureType': 'poi.school', 'stylers': [{'color': '#a95521'}, {'lightness': 35}]}, {}, {'featureType': 'poi.medical', 'elementType': 'geometry.fill', 'stylers': [{'color': '#813033'}, {'lightness': 38}, {'visibility': 'off'}]}, {}, {}, {}, {}, {}, {}, {}, {}, {}, {}, {}, {'elementType': 'labels'}, {'featureType': 'poi.sports_complex', 'stylers': [{'color': '#9e5916'}, {'lightness': 32}]}, {}, {'featureType': 'poi.government', 'stylers': [{'color': '#9e5916'}, {'lightness': 46}]}, {'featureType': 'transit.station', 'stylers': [{'visibility': 'off'}]}, {'featureType': 'transit.line', 'stylers': [{'color': '#813033'}, {'lightness': 22}]}, {'featureType': 'transit', 'stylers': [{'lightness': 38}]}, {'featureType': 'road.local', 'elementType': 'geometry.stroke', 'stylers': [{'color': '#f19f53'}, {'lightness': -10}]}, {}, {}, {}]"
                                        {% when 'subtle' %}
